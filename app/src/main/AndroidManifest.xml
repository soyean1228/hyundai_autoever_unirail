<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.UNIRAIL">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/esicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service
            android:name="com.UNIRAIL.UNIRAIL_service.UNIRAIL_service"
            android:enabled="true"
            android:exported="true"></service>

        <activity
            android:name="com.UNIRAIL.SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.UNIRAIL.MainActivity"></activity>
        <activity android:name="com.UNIRAIL.TrafficSubwayInfo" />
        <activity android:name="com.UNIRAIL.StartSearchActivity" />
        <activity android:name="com.UNIRAIL.FinalSearchActivity" />
        <activity android:name="com.UNIRAIL.SettingActivity" />
        <activity android:name="com.UNIRAIL.searchRoute" />
        <activity android:name="com.UNIRAIL.SearchRouteResult" />
        <activity android:name="com.UNIRAIL.RecentRoute" />
        <activity android:name="com.UNIRAIL.SearchMap" />
        <activity android:name="com.UNIRAIL.StationMapActivity" />
    </application>

</manifest>